{
  "manifest_version":2,
  "name":"Wordle Unlimited Solver",
  "version":"1.0.0",
  "content_scripts": [
    {
      "matches":["https://wordleunlimited.org/*"],
      "js":["wordleSolver.js"],
      "run_at":"document_idle"
    }

  ],
  "browser_action":{
      "browser_style":true,
      "default_icon":{
          "96":"wordle.png"
      },
      "default_title":"Wordle Solver"
  },
  "permissions":[
      "<all_urls>"   
  ]
}